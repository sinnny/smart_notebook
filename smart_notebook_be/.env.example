# Prefer a single URL (works well with Supabase / Vercel)
DATABASE_URL=postgresql://USER:PASSWORD@HOST:5432/DBNAME?sslmode=require

# Or set discrete values instead of DATABASE_URL
# POSTGRES_HOST=
# POSTGRES_PORT=5432
# POSTGRES_DB=
# POSTGRES_USER=
# POSTGRES_PASSWORD=
# POSTGRES_SSLMODE=require

# Required for OpenAI features
OPENAI_API_KEY=

# Optional
# CORS_ORIGINS=http://localhost:3000,http://localhost:5173
# API_PREFIX=/api

